dimensions:
        bio_time = 242 ;
        s_rho = 36 ;
        eta_rho = 359 ;
        xi_rho = 336 ;
        time = UNLIMITED ; // (3689 currently)
variables:
        double NH4(bio_time, s_rho, eta_rho, xi_rho) ;
                NH4:long_name = "ammonium concentration" ;
                NH4:units = "millimole_ammonium meter-3" ;
                NH4:field = "NH4, scalar, series" ;
                NH4:time = "bio_time" ;
        double NO3(bio_time, s_rho, eta_rho, xi_rho) ;
                NO3:long_name = "nitrate concentration" ;
                NO3:units = "millimole_nitrate meter-3" ;
                NO3:field = "NO3, scalar, series" ;
                NO3:time = "bio_time" ;
        double PO4(bio_time, s_rho, eta_rho, xi_rho) ;
                PO4:long_name = "P concentration" ;
                PO4:units = "millimole_P meter-3" ;
                PO4:field = "PO4, scalar, series" ;
                PO4:time = "bio_time" ;
        double SiOH4(bio_time, s_rho, eta_rho, xi_rho) ;
                SiOH4:long_name = "silicate concentration" ;
                SiOH4:units = "millimole_silicate meter-3" ;
                SiOH4:field = "SiOH4, scalar, series" ;
                SiOH4:time = "bio_time" ;
        double TIC(bio_time, s_rho, eta_rho, xi_rho) ;
                TIC:long_name = "total inorganic carbon" ;
                TIC:units = "millimole_carbon meter-3" ;
                TIC:field = "TIC, scalar, series" ;
                TIC:time = "bio_time" ;
        double alkalinity(bio_time, s_rho, eta_rho, xi_rho) ;
                alkalinity:long_name = "total alkalinity" ;
                alkalinity:units = "milliequivalents meter-3" ;
                alkalinity:field = "alkalinity, scalar, series" ;
                alkalinity:time = "bio_time" ;
        double bio_time(bio_time) ;
                bio_time:units = "days since 1858-11-17 00:00:00 GMT" ;
                bio_time:field = "bio_time, scalar, series" ;
        double ocean_time(time) ;
                ocean_time:long_name = "subsurface temp/salt observations time" ;
                ocean_time:units = "days since 1858-11-17 00:00:00" ;
                ocean_time:field = "ocean_time, scalar, series" ;
        float salt(time, s_rho, eta_rho, xi_rho) ;
                salt:long_name = "salinity" ;
                salt:units = "PSU" ;
                salt:field = "salinity, scalar, series" ;
                salt:time = "ocean_time" ;
        float temp(time, s_rho, eta_rho, xi_rho) ;
                temp:long_name = "potential temperature" ;
                temp:units = "Celcius" ;
                temp:field = "temperature, scalar, series" ;
                temp:time = "ocean_time" ;
        double pH(bio_time, s_rho, eta_rho, xi_rho) ;
                pH:long_name = "P concentration" ;
                pH:units = "millimole_P meter-3" ;
                pH:field = "PO4, scalar, series" ;
                pH:time = "bio_time" ;
        double DOC(bio_time, s_rho, eta_rho, xi_rho) ;
                DOC:long_name = "total inorganic carbon" ;
                DOC:units = "millimole_carbon meter-3" ;
                DOC:field = "TIC, scalar, series" ;
                DOC:time = "bio_time" ;
